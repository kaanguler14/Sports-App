<?import javafx.scene.control.TextField?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.paint.Color?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="523.0" prefWidth="520.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo.RegisterController">
   <children>
      <TextField fx:id="lastnameTextField" layoutX="96.0" layoutY="187.0" prefHeight="26.0" prefWidth="362.0" promptText="Lastname" />
      <Label layoutX="96.0" layoutY="277.0" text="Password" />
      <AnchorPane prefHeight="82.0" prefWidth="520.0" style="-fx-background-color: #8B008B;">
         <children>
            <Label layoutX="95.0" layoutY="27.0" text="User Registrations" textFill="WHITE">
               <font>
                  <Font size="18.0" />
               </font>
               <effect>
                  <DropShadow>
                     <color>
                        <Color red="1.0" green="1.0" blue="1.0" />
                     </color>
                  </DropShadow>
               </effect>
            </Label>
         </children>
      </AnchorPane>
      <Label layoutX="96.0" layoutY="115.0" text="Firstname" />
      <TextField fx:id="firstnameTextField" layoutX="96.0" layoutY="133.0" prefHeight="26.0" prefWidth="362.0" promptText="Firstname" />
      <Label layoutX="96.0" layoutY="169.0" text="Lastname" />
      <Label layoutX="96.0" layoutY="221.0" text="Username" />
      <TextField fx:id="usernameTextField" layoutX="96.0" layoutY="241.0" prefHeight="26.0" prefWidth="362.0" promptText="Username" />
