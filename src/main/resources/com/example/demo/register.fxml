      <Button fx:id="registerButton" layoutX="96.0" layoutY="392.0" mnemonicParsing="false" onAction="#registerButtonOnAction" prefHeight="26.0" prefWidth="362.0" style="-fx-background-color: #8B008B;" text="Register" textFill="WHITE">
         <font>
            <Font size="13.0" />
         </font>
      </Button>
      <Button fx:id="backLoginButton" layoutX="96.0" layoutY="432.0" mnemonicParsing="false" onAction="#setBackLoginButton" prefHeight="26.0" prefWidth="362.0" style="-fx-background-color: #8B008B;" text="Back Login Page" textFill="WHITE">
         <font>
            <Font size="13.0" />
         </font>
      </Button>
      <Button fx:id="closeButton" layoutX="96.0" layoutY="472.0" mnemonicParsing="false" onAction="#setCloseButton" prefHeight="26.0" prefWidth="362.0" style="-fx-background-color: #8B008B;" text="Close" textFill="WHITE">
         <font>
            <Font size="13.0" />
         </font>
      </Button>
      <Label fx:id="registrationMessageLabel" alignment="CENTER" layoutX="4.0" layoutY="92.0" prefHeight="18.0" prefWidth="514.0" textFill="#8800ff">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <Label fx:id="confirmPasswordLabel" alignment="CENTER" layoutX="95.0" layoutY="329.0" prefHeight="18.0" prefWidth="426.0" textFill="RED">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
